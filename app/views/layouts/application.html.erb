<!DOCTYPE html>
<html>
<head>
  <title>Rainforest</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

  <% flash.each do |name, message| %>
    <%= content_tag :div, message, class: name %>
  <% end %>

<%= yield %>

</body>
</html>
